<?xml version="1.0" encoding="utf-8" ?>
<meta>
  <info name="WiredPlayers" author="Xabi" type="gamemode"/>
  
  <!-- Gamemode library -->
  <script src="bin/Debug/netcoreapp2.2/VNRPG.dll" />

  <settings>
    <!-- Database host, 120.0.0.1 if localhost -->
    <setting name="host" value="127.0.0.1"/>
    <!-- Database username -->
    <setting name="username" value="gta"/>
    <!-- Database password -->
    <setting name="password" value="gta5server-WP"/>
    <!-- Database name -->
    <setting name="database" value="gtav"/>
    <!-- Can take NONE or REQUIRED values -->
    <setting name="ssl" value="required"/>

    <!-- Gamemode language settings -->
    <setting name="culture" value="en-US"/>
  </settings>

</meta>
